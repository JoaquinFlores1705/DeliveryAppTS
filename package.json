{
  "name": "deliveryapp",
  "version": "1.0.0",
  "description": "BackEnd deliveryapp",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/app.ts",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs -d src/config/db.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d src/config/db.t migration:revert",
    "schema:sync": "npm run typeorm schema:sync -- -d src/config/db.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/config/db.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/db.ts",
    "migration:create": "npm run typeorm migration:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "typeorm": "^0.3.19",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  }
}
